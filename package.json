{
  "name": "nedolingo",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "A learning quiz app inspired by Duolingo",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "pnpm build && gh-pages -d dist",
    "pages:enable": "gh api repos/harnyk/nedolingo/pages -X POST -f source[branch]=gh-pages -f source[path]=/",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug"
  },
  "keywords": [
    "quiz",
    "learning",
    "education",
    "react",
    "vite"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.25.0",
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "vite": "^7.3.1",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "js-yaml": "^4.1.1",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.12.0"
  }
}
